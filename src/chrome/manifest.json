{
  "manifest_version": 2,
  "name": "BudgetBuddy",
  "version": "1.1.2",
  "description": "Analysis tool for UCSD financial accounts",
  "content_scripts": [
    {
      "matches": [
        "*://eacct-ucsd-sp.transactcampus.com/eAccounts/AccountTransaction.aspx*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "bb.css"
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "bb16.png",
    "48": "bb48.png",
    "128": "bb128.png"
  }
}