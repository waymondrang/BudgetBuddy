{
    "name": "budget buddy",
    "version": "0.12.0",
    "description": "browser extension for budgeting Â© raymond wang",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://eacct-ucsd-sp.transactcampus.com/eAccounts/AccountTransaction.aspx"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "bb.css"
            ],
            "matches": [
                "*://eacct-ucsd-sp.transactcampus.com/*"
            ]
        }
    ],
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    }
}