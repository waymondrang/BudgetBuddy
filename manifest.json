{
    "name": "budget buddy",
    "version": "0.14.0",
    "description": "browser extension for budgeting ucsd financial accounts Â© raymond wang",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "*://eacct-ucsd-sp.transactcampus.com/eAccounts/AccountTransaction.aspx"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "browser_action": {},
    "web_accessible_resources": [
        "bb.css"
    ],
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": { "16": "bb16.png",
        "48": "bb48.png",
       "128": "bb128.png" }
}